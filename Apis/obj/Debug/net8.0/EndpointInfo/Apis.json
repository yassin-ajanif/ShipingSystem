            "nullable": true
          },
          "shippingTypeEName": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ShippmentDto": {
        "required": [
          "receiverId",
          "senderId",
          "shippingTypeId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "senderId": {
            "type": "string",
            "format": "uuid"
          },
          "senderName": {
            "type": "string",
            "nullable": true
          },
          "receiverId": {
            "type": "string",
            "format": "uuid"
          },
          "receiverName": {
            "type": "string",
            "nullable": true
          },
          "shippingTypeId": {
            "type": "string",
            "format": "uuid"
          },
          "shippingTypeName": {
            "type": "string",
            "nullable": true
          },
          "sender": {
            "$ref": "#/components/schemas/UserSenderDto"
          },
          "receiver": {
            "$ref": "#/components/schemas/UserReceiverDto"
          },
          "shippingType": {
            "$ref": "#/components/schemas/ShippingTypeDto"
          },
          "subscriptionPackage": {
            "$ref": "#/components/schemas/SubscriptionPackageDto"
          },
          "paymentMethod": {
            "$ref": "#/components/schemas/PaymentMethodDto"
          },
          "paymentMethodId": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "paymentMethodName": {
            "type": "string",
            "nullable": true
          },
          "packageValue": {
            "minimum": 0,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "shippingRate": {
            "minimum": 0,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "shippingDate": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ShippmentDtoApiResponse": {
        "type": "object",
        "properties": {
          "success": {
            "type": "boolean"
          },
          "message": {
            "type": "string",
            "nullable": true
          },
          "data": {
            "$ref": "#/components/schemas/ShippmentDto"
          },
          "errors": {
            "type": "array",
