        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "receiverName": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string"
          },
          "email": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string",
            "format": "email",
            "nullable": true
          },
          "phone": {
            "maxLength": 200,
            "minLength": 0,
