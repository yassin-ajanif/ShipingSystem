          "packageName": {
            "maxLength": 200,
            "minLength": 0,
            "type": "string"
          },
          "price": {
            "minimum": 0,
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "durationInDays": {
            "maximum": 2147483647,
            "minimum": 1,
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "description": {
